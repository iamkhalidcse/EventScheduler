<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.khalidjavaid.bgsbu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.khalidjavaid.bgsbu.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.khalidjavaid.bgsbu.aboutus"
            android:parentActivityName="com.example.khalidjavaid.bgsbu.MainActivity">
            <intent-filter>
                <action android:name="com.example.khalidjavaid.bgsbu.aboutus" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.khalidjavaid.bgsbu.Directory" />
        <activity android:name="com.example.khalidjavaid.bgsbu.Cse" />
        <activity android:name="com.example.khalidjavaid.bgsbu.Ere" />
        <activity android:name="com.example.khalidjavaid.bgsbu.Civil" />
        <activity android:name="com.example.khalidjavaid.bgsbu.Ece" />
        <activity android:name="com.example.khalidjavaid.bgsbu.Ite"></activity>
    </application>

</manifest>